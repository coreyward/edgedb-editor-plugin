{
    "name": "edgedb",
    "displayName": "edgedb",
    "description": "Grammars for the EdgeQL and ESchema languages in EdgeDB",
    "version": "0.0.1",
    "publisher": "magicstack",
    "engines": {
        "atom": ">=1.0.0 <2.0.0",
        "vscode": ">=1.19.0,<=2.0.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "edgeql",
                "aliases": [
                    "EdgeQL",
                    "edgeql"
                ],
                "extensions": [
                    ".eql"
                ],
                "configuration": "./settings/vscode/edgeql.json"
            },
            {
                "id": "edgeschema",
                "aliases": [
                    "EdgeSchema",
                    "eschema"
                ],
                "extensions": [
                    ".eschema"
                ],
                "configuration": "./settings/vscode/eschema.json"
            }
        ],
        "grammars": [
            {
                "language": "edgeql",
                "scopeName": "source.edgeql",
                "path": "./grammars/edgeql.tmLanguage"
            },
            {
                "language": "edgeschema",
                "scopeName": "source.edgeschema",
                "path": "./grammars/eschema.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "syntaxdev": "0.0.16"
    }
}
